# This method accepts an array and returns a modified array with duplicate elements removed.

class Array
    def my_uniq(array)
# Create a new empty array
        modified_array = []
# Iterate over the array to check for duplicate elements
        array.each do |i|
            modified_array << i if modified_array.include?(i) == false   
            end
# Return the modified array
        modified_array
    end
end
